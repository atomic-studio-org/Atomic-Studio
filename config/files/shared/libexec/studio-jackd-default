#!/usr/bin/env bash 
pulseaudio -k
pactl load-module module-jack-source
jack_control start
jack_control ds alsa
jack_control dps rate 48000
jack_control dps nperiods 2
jack_control dps period 64
sleep 5sec
a2j_control --ehw
a2j_control --start
sleep 5sec
qjackctl &
pactl load-module module-jack-sink
